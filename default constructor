public class student{
    String name;
    int age;
    
    
    public student(){
        name="sugan";
        age=8;
    }
        
     void displayinfo(){  
         System.out.println("Name:"+name);
         System.out.println("age:"+age);
    }    
     public static void main (String args[]){
         student s1=new student();
         s1.displayinfo();
     }   
}
